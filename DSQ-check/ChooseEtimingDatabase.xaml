<Window x:Class="DSQ_check.ChooseEtimingDatabase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Velg eTiming database" Height="175" Width="348" MinHeight="150" MinWidth="250" Closing="Window_Closing">
    <Grid>
        <TextBox Height="23" Margin="12,41,93,0" Name="textbox_filepath" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBlock1" Text="Velg hvor databasen ligger" VerticalAlignment="Top" />
        <Button Content="Bla gjennom" Height="23" HorizontalAlignment="Right" Margin="0,41,12,0" Name="button_choose" VerticalAlignment="Top" Width="75" Click="button_choose_Click" />
        <Button Content="Åpne database" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="button_open" VerticalAlignment="Bottom" Width="94" Click="button_open_Click" />
        <Button Content="Avbryt" Height="23" HorizontalAlignment="Left" Margin="12,0,0,12" Name="button_cancel" Click="button_cancel_Click" VerticalAlignment="Bottom" Width="94" />
    </Grid>
</Window>
