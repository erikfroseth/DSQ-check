<Window x:Class="DSQ_check.ChooseEventSysEvent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Velg event" Height="240" Width="220" MinHeight="240" MinWidth="220" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid>
        <ComboBox Height="23" Margin="12,39,12,0" Name="combobox_database" VerticalAlignment="Top" SelectionChanged="combobox_database_SelectionChanged" />
        <TextBlock Height="23" Margin="12,12,12,0" Name="textBlock1" Text="Velg database" VerticalAlignment="Top" TextAlignment="Center" />
        <TextBlock Height="23" Margin="12,88,12,0" Name="textBlock2" Text="Velg event" TextAlignment="Center" VerticalAlignment="Top" />
        <Button Content="Avbryt" Height="23" HorizontalAlignment="Left" Margin="12,0,0,12" Name="button_close" Click="button_close_Click" VerticalAlignment="Bottom" Width="75" />
        <Button Content="Åpne" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="button_open" Click="button_open_Click" VerticalAlignment="Bottom" Width="75" IsEnabled="False" />
        <ComboBox Height="23" Margin="12,117,12,0" Name="combobox_event" VerticalAlignment="Top" DisplayMemberPath="Value" SelectedValuePath="Key" IsEnabled="False" SelectionChanged="combobox_event_SelectionChanged" />
    </Grid>
</Window>
